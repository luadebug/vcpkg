@PACKAGE_INIT@

    find_dependency

if(NOT TARGET unofficial::mapbox-variant::variant)
    add_library(unofficial::mapbox-variant::variant INTERFACE IMPORTED)
    set_target_properties(unofficial::mapbox-variant::variant PROPERTIES
        INTERFACE_INCLUDE_DIRECTORIES "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_INCLUDEDIR@"
        INTERFACE_COMPILE_FEATURES "cxx_std_11"
    )
    if(APPLE)
        set_property(TARGET unofficial::mapbox-variant::variant APPEND PROPERTY
            INTERFACE_LINK_OPTIONS "-framework CoreFoundation"
        )
    endif()
    if(UNIX AND NOT APPLE)
        set_property(TARGET unofficial::mapbox-variant::variant APPEND PROPERTY
            INTERFACE_LINK_LIBRARIES "rt"
        )
    endif()
endif()

include(CMakeFindDependencyMacro)

find_dependency(Boost CONFIG REQUIRED COMPONENTS timer system chrono)

check_required_components(unofficial-mapbox-variant)
