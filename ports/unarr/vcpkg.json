{
  "name": "unarr",
  "version": "1.1.1",
  "description": "A decompression library for rar, tar, zip and 7z archives.",
  "homepage": "https://github.com/selmf/unarr",
  "license": "LGPL-3.0-only",
  "dependencies": [
    "bzip2",
    "liblzma",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zlib"
  ],
  "features": {
    "enable-7z": {
      "description": "Build with 7z support",
      "dependencies": [
        "7zip"
      ]
    }
  }
}
